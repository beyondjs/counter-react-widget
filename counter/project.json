{
  "version": "1.0.7",
  "scope": "beyond-js",
  "name": "counter-react-widget",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "./modules"
  },
