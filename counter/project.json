{
  "version": "1.0.3",
  "scope": "beyond-js",
  "name": "counter-react-widget",
  "modules": {
    "path": "./modules"
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "environment": "development",
        "ports": {
          "bundles": 6500
        }
      },
      {
        "name": "tsc",
        "platform": "web",
        "environment": "development",
        "ts": {
          "compiler": "tsc"
        },
        "ports": {
          "bundles": 6501
        }
      },
      {
        "name": "npm",
        "npm": {
          "platforms": {
            "web": "web"
          }
        }
      }
    ]
  }
}